{
	"compilerOptions": {
	  "outDir": "./build",
	  "target": "ES6",
	  "lib": ["es2021"],
	  "module": "commonjs",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["./src/**/*"],
	"exclude": ["./node_modules", "**/*.spec.ts"]
  }
  